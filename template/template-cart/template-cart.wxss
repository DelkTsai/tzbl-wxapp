page{
    /*height: 100%;*/
}
.container{
    justify-content:initial;
}

.title-box{
    width: 100%;
    padding-top: 200rpx;
    text-align: center;
    font-size:28rpx;
    color:#999;
    margin-bottom: 50rpx;
    display: inline-flex;
    flex-direction: column;
    align-items: center;
}

.title-box .cart{
  width: 140rpx;
  height: 140rpx;   
}

.to-index-btn{
   display:  flex;
   flex-direction: column;
   align-items: center;
   margin-top: 100rpx;
   color: #f2d085
}

.to-index-btn .code{
    width: 150rpx;
    height: 150rpx;
    margin-bottom: 20rpx;
}
.list-top{
    width: 100%;
    height: 88rpx;
    line-height: 88rpx;
    box-sizing: border-box;
    display: flex;
    justify-content:space-between;
    padding: 0 30rpx;
    font-size: 28rpx;
    align-items: center;
}
.list-top .label{
    color: #000;
}
.list-top .edit-btn{
    color: #999;
    height: 100%;
}
.goodsList{
    width: 100%;
    background-color: #fff;
}
.a-gooods{
    width: 100%;
    overflow: hidden;

}
.a-goods-conts{
    display: flex;
    justify-content:space-between;
    padding-left: 30rpx;
    width: 870rpx;
    box-sizing: border-box;
    transition: margin-left 0.2s ease-in-out; 
}
.goods-info{
    flex: 1;
    display: flex;
    flex-direction: row;
    justify-content:space-between;
    padding: 20rpx;
}
.goods-info .img-box{
    width: 90rpx;
    height:90rpx;
    overflow: hidden;
    margin-right: 20rpx;
    background-color: #d8d8d8;
}
.goods-info .text-box{
    width: 240rpx;
    position: relative;
    display: flex;
    flex-direction: column;
    height: 90rpx;
}
.goods-info .text-box .goods-title{
    font-size: 28rpx;
    color:#000;
    text-overflow: ellipsis;
    white-space: nowrap;
    overflow: hidden;
}
.goods-info .text-box .goods-label{
    font-size: 26rpx;
    color:#999;
    height: 38rpx;
    line-height: 38rpx;
}
.goods-info .text-box .goods-label .goods-price{
    font-size: 26rpx;
    color:#000000;
    margin-left: 40rpx;
}
.goods-info .buy-num{
    width: 250rpx;
    height: 90rpx;
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: space-around;
    line-height: 1;
}
.goods-info .buy-num .icon{
  width: 60rpx;
  height: 60rpx;
  margin-right:0rpx; 
}
.goods-info .text-box .buy-num .jian-btn{
    width: 48rpx;
    height: 100%;
    border-left: 1rpx solid #ccc;
    border-bottom: 1rpx solid #ccc;
    border-top: 1rpx solid #ccc;
    border-bottom-left-radius: 6rpx;
    border-top-left-radius: 6rpx;
}
.goods-info .text-box .buy-num .jian-btn.disabled{
    background-color: #f5f5f9;
    border-left: 1rpx solid #eee;
    border-bottom: 1rpx solid #eee;
    border-top: 1rpx solid #eee;
    color: #ccc;
}
.goods-info .text-box .buy-num .jia-btn{
    width: 48rpx;
    height: 100%;
    border-right: 1rpx solid #ccc;
    border-bottom: 1rpx solid #ccc;
    border-top: 1rpx solid #ccc;
    border-bottom-right-radius: 6rpx;
    border-top-right-radius: 6rpx;
}
.goods-info .text-box .buy-num .jia-btn.disabled{
    background-color: #f5f5f9;
    border-right: 1rpx solid #eee;
    border-bottom: 1rpx solid #eee;
    border-top: 1rpx solid #eee;
    color: #ccc;
}
.goods-info .text-box .buy-num input{
    width: 68rpx;
    height: 48rpx;
    min-height: 48rpx;
    text-align: center;
    font-size: 24rpx;
    border: 1rpx solid #ccc;
}

.goods-info .img-box .img{
    width: 160rpx;
    height:160rpx;
}

.a-goods-conts .delete-btn{
    width: 120rpx;
    line-height: 130rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    text-align: center;
    background:#e64340;
    font-size: 24rpx;
    color: #fff;
  
}




.cart-tab-wrp{
  height: 100rpx;
  position: fixed;
  bottom: 0rpx;
  background: rgba(255, 255, 255, 0.9);
  display: flex;
  flex-direction:row;
  width: 100%;
  z-index: 10;
  color: black
}
.cart-item-total{
  padding-left: 30rpx;
}
.cart-item-icon-wrp{
  width: 90rpx;
  height: 90rpx;
  position: absolute;
  top:-5rpx;
  background-color: #f8b62b;
  border-radius: 50%;
}
.cart-item-icon{
  width: 90rpx;
  height: 90rpx;

}
.cart-item-total-num{
    display: inline-block;
    width: 36rpx;
    height: 36rpx;
    font-size: 24rpx;
    color: white;
    line-height: 36rpx;
    border-radius: 36rpx;
    text-align: center;
    position: absolute;
    right: 0rpx;
    background-color: #373934;
}
.cart-item-total-price{
    display: inline-block;
    width: 300rpx;
    height: 40rpx;
    font-size: 30rpx;
    line-height: 100rpx;
    padding-left: 110rpx;
}
.cart-tab-item-btn{
    width: 300rpx;
    display: block;
    height: 100rpx;
    line-height: 100rpx;
    font-size: 36rpx;
    color: #fff;
    float: right;
    text-align: center;
    background-color: #f8b62b;

}
.cart-tab-item{
  width: 375rpx;
}
.gosend{
  position: absolute;
  bottom:380rpx;
  left: 50%;
  width: 150rpx;
  height: 150rpx;
  margin-top: -75rpx;
  margin-left: -75rpx;
}